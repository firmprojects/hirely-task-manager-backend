{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "PRISMA_CLIENT_ENGINE_TYPE": "dataproxy"
  },
  "buildCommand": "prisma generate && next build",
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": ["OPTIONS", "GET", "POST", "PUT", "DELETE"],
      "dest": "/api/$1"
    }
  ]
}
